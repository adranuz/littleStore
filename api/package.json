    "sequelize-cli": "^6.4.1"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "faker": "5.5.3",
    "helmet": "^5.0.2",
